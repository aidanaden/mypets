{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "unpaid",
        "processing",
        "shipping",
        "delivered"
      ]
    },
    "total_price": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "products": {
      "collection": "product"
    },
    "user": {
      "via": "orders",
      "plugin": "users-permissions",
      "model": "user"
    },
    "order_date": {
      "type": "date"
    },
    "delivery_date": {
      "type": "date"
    }
  }
}
